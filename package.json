{
  "name": "twittxr",
  "version": "0.2.0",
  "description": "Extract information from Twitter via the Syndication API without authentication.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/types.js",
  "scripts": {
    "test": "npm run build:esm && node test.js",
    "clean": "npx rimraf dist",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --outDir dist/esm --declaration true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Owen3H/twittxr.git"
  },
  "keywords": [
    "twitter",
    "tweet",
    "xdotcom",
    "api-wrapper",
    "twitter-api",
    "syndication",
    "scraper"
  ],
  "author": "Owen3H",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Owen3H/twittxr/issues"
  },
  "homepage": "https://github.com/Owen3H/twittxr#readme",
  "dependencies": {
    "tslib": "^2.6.1",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6"
  }
}
