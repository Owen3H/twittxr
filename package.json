{
  "name": "twittxr",
  "version": "0.5.2",
  "description": "Extract information from Twitter via the Syndication API without authentication.",
  "type": "module",
  "main": "dist/umd.cjs",
  "module": "dist/esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run --config ./vite.config.ts",
    "test-file": "pnpm run build && node src/test.js",
    "clean": "npx rimraf dist",
    "build": "pnpm run clean && npx rollup -c && tsc --declaration true --emitDeclarationOnly",
    "lint": "eslint .",
    "ci": "pnpm run lint && pnpm run test && pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Owen3H/twittxr.git"
  },
  "keywords": [
    "twitter",
    "tweet",
    "xdotcom",
    "api-wrapper",
    "twitter-api",
    "syndication",
    "scraper"
  ],
  "author": "Owen3H",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Owen3H/twittxr/issues"
  },
  "homepage": "https://github.com/Owen3H/twittxr#readme",
  "dependencies": {
    "tslib": "latest",
    "undici-shim": "^1.5.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=10"
  }
}
